{
  "name": "<%=moduleName%>",
  "version": "0.10.0",
  "description": "Generate a <%=generatorName%> for Sails.",
  "main": "gen/index.js",
  "scripts": {
    "test": "mocha"
  },
  "keywords": [
    "<%=generatorName%>",
    "generator",
    "sails",
    "generate",
    "plugin"
  ],
  "author": "<%= author %>",
  "license": "MIT",
  "dependencies": {
  	"lodash": ">=2.4.x",
  	"merge-defaults": ">=0.1.0"
  },
  "devDependencies": {
    "sails-generate": "*",
    "reportback": "*",
    "fs-extra": "*",
    "root-require": ">=0.2.x"
  },
  "sails": {
  	"generator": {
	 		"name": "<%= generatorName %>",
	 		"behavior": "overrides `sails generate <%= generatorName %>`",
	 		"sailsVersion": "~0.10.0",
	 		"main": "lib/index"
  	}
  }
}
